{
  "[javascript]": {
    "editor.formatOnSave": false
  },
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "files.associations": {
    "*.js": "javascriptreact"
  },
  "files.autoSave": "onFocusChange",
  "prettier.disableLanguages": ["js"],
  "window.zoomLevel": 5,
  "editor.fontSize": 12,
  "terminal.integrated.fontSize": 16,
  "editor.tabSize": 2,
}
